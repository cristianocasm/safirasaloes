<%= form_for @schedule, remote: true, autocomplete: "off", onsubmit: "" do |f| %>
  <div class="field">
    <%= f.label :nome %><br>
    <%= f.text_field :nome, class: "form-control", autocomplete: "off" %>
  </div>
  <div class="field">
    <%= f.label :email %><br>
    <%= f.text_field :email, class: 'twitter-typeahead form-control', autocomplete: "off" %>
  </div>
  <div class="field">
    <%= f.label :telefone %><br>
    <%= f.text_field :telefone, class: "twitter-typeahead form-control", autocomplete: "off" %>
  </div>
  <div class="field">
    <%= f.label :serviço %><br>
    <%= f.select :service_id, current_professional.services.collect {|p| [ p.nome, p.id ] } %>
  </div>
  <div class="field">
    <%= f.label :início %><br>
    <%= f.datetime_select :datahora_inicio %>
  </div>
  <div class="field">
    <%= f.label :fim %><br>
    <%= f.datetime_select :datahora_fim %>
  </div>
  <%= f.hidden_field :customer_id %>
  <%= f.hidden_field :id %>
  <div class="actions">
    <%= f.submit 'Marcar Horário', data: { disable_with: "Aguarde..." } %>
  </div>
<% end %>