            <section id="page-content" style="margin-left: 0px !important">

                <!-- Start page header -->
                <div class="header-content" style="display: inherit">
                    <h2>
                        <%= @professional.nome %> <span>Galeria de Fotos</span>
                            <% if @professional.whatsapp.present? %>
                                <div class='observacao'>
                                    <i class='fa fa-whatsapp'></i> <%= @professional.whatsapp %>
                                </div>
                            <% end %>
                            <% if @professional.telefone.present? %>
                                <div class='observacao'>
                                    <i class='fa fa-phone'></i> <%= @professional.telefone %>
                                </div>
                            <% end %>
                    </h2>
                </div><!-- /.header-content -->
                <!--/ End page header -->

                <!-- Start body content -->
                <div class="body-content animated fadeIn">

